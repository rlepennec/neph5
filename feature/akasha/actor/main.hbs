<div class="akasha">

    <div class="column">

        <div class="header">

            <img class="puce"/>

            <img class="separator"/>

            <label class="name">{{localize "NEPH5E.akasha"}}</label>

        </div>

        <div class="directions">

            <div class="vaisseau nef tooltip" data-type="nef">
                <img class="activate {{#unless actor.system.akasha.nef.active}}undeveloped{{/unless}}"/>
                <label class="tooltiptext">{{localize "NEPH5E.nef"}}</label>
            </div>

            <div class="zenith">
                <img class="circle"/>
                <input
                    {{#if actor.locked}}disabled{{/if}}
                    name="system.akasha.boussole.zenith"
                    type="text"
                    value="{{system.akasha.boussole.zenith}}"
                    data-dtype="Number"/>
                <label>Zenith +{{getLevel actor.system.akasha.boussole.zenith}}</label>
