<div class="laboratoire">

    <div class="laboratoires">

        {{#with (laboratoryOwner actor) as |actor|}}
        <label class="zone">Laboratoire de {{actor.name}}</label>
        {{/with}}

        {{#unless (isEmptyCollection actor.laboratoires)}}
        <ol class="menu">

            {{#each actor.laboratoires as |item|}}
            {{#if (ne ../actor.system.alchimie.courant item.id)}}    
            <li class="name select"data-sid="{{item.id}}">{{item.name}}</li>
            {{/if}}
            {{/each}}

            {{#if (nonNull actor.system.alchimie.courant)}}
            <li class="name select">{{actor.name}}</li>
            {{/if}}

            {{#each actor.laboratoires as |item|}}
            {{#if (eq ../actor.system.alchimie.courant item.id)}} 
            <li class="name delete">
                <label>{{item.name}}</label>
                <i class="fas fa-trash"></i>
            </li>
            {{/if}}
            {{/each}}

        </ol>
        {{/unless}}

    </div>

